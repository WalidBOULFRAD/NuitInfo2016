{
	"name": "web-template-webpack",
	"version": "0.1.0",
	"description": "A simple AngularJS 1.5 with tests karma and protractor boilerplate using ES6 and components, material design and webpack",
	"main": "index.js",
	"scripts": {
		"test:pre:e2e": "webdriver-manager update && webdriver-manager start",
		"test:e2e": "protractor .protractor.conf.js",
		"test:watch:e2e": "protractor .protractor.conf.js --elementExplorer",
		"test": "karma start && protractor .protractor.conf.js",
		"test:unit": "karma start",
		"xo": "eslint lib/**/*.js",
		"prod": "rm -rf dist/* && cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
		"dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.config.js",
		"server:prod": "http-server dist --cors",
		"test:unit:watch": "karma start --auto-watch --no-single-run"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kevincaradant/web-template-webpack.git"
	},
	"keywords": [
		"angular",
		"mvc",
		"angular1.5",
		"webpack",
		"es6",
		"babel",
		"protractor",
		"bootstrap",
		"boilerplate",
		"eslint",
		"jasmine",
		"xo",
		"sass",
		"karma",
		"material",
		"isparta"
	],
	"author": "KÃ©vin CARADANT <kevin.caradant@gmail.com> (https://caradantkevin.fr/)",
	"license": "MIT",
